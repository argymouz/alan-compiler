main () : proc
   a : byte;
   x : int;
   y : int;
   sum : int;
{
   x = 10;
   y = 0;
   a = '\x01';
   sum = 0;

   while (a != '\0')
      if (a != '\0' & x>=y) {
         sum = sum + x * y;
         x = x-1;
      }
      else if (true) {
         if (y <= 10)
            x = 10;
         else if (! false)
            a = '\0';
         y = y + 1;
      }
   if (sum == 1705) writeString("42\n"); else writeString("wrong!!!\n");
}
